{
  "name": "uppy-bot",
  "version": "1.0.0",
  "type": "module",
  "description": "Uppy bot",
  "main": "dist/index.js",
  "scripts": {
    "format:fix": "prettier --write \"./**/*.ts\"",
    "format:check": "prettier --check \"./**/*.ts\"",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate:create": "pnpm migrate create",
    "migrate:up": "pnpm migrate up",
    "migrate:down": "pnpm migrate down",
    "build": "tsc -b && tsc-alias",
    "dev:ds": "nodemon --config nodemon.discord.json",
    "dev:sr": "nodemon --config nodemon.server.json",
    "dev:tg": "nodemon --config nodemon.telegram.json",
    "start:ds": "node dist/discord/main.js",
    "start:sr": "node dist/server/main.js",
    "start:tg": "node dist/telegram/main.js",
    "test:ci": "vitest --no-watch"
  },
  "imports": {
    "#/*": "./dist/*"
  },
  "license": "ISC",
  "homepage": "",
  "dependencies": {
    "@discordx/importer": "^1.3.3",
    "@discordx/pagination": "4.3.0",
    "@discordx/utilities": "^8.0.0",
    "@grammyjs/auto-retry": "^2.0.2",
    "@grammyjs/commands": "^1.2.0",
    "@grammyjs/emoji": "^1.2.0",
    "@grammyjs/hydrate": "^1.6.0",
    "@grammyjs/menu": "^1.3.1",
    "@napi-rs/canvas": "^0.1.81",
    "@ts-fetcher/cache": "^1.5.2",
    "@ts-fetcher/rest": "^1.4.4",
    "@typegoose/typegoose": "^12.18.0",
    "amqplib": "^0.10.9",
    "amqplib-rpc": "^3.1.0",
    "axios": "^1.12.2",
    "ayologger": "^0.0.11",
    "crypto-js": "^4.2.0",
    "discord.js": "^14.24.1",
    "discordx": "^11.12.5",
    "dotenv": "^17.2.1",
    "fastify": "^5.6.2",
    "glob": "^11.0.3",
    "grammy": "^1.38.3",
    "ioredis": "^5.8.2",
    "luxon": "^3.7.1",
    "mongoose": "^8.19.2",
    "node-html-parser": "^7.0.1",
    "node-schedule": "^2.1.1",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@ts-fetcher/types": "^1.5.3",
    "@types/amqplib": "^0.10.8",
    "@types/crypto-js": "^4.2.2",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.1.0",
    "@types/node-schedule": "^2.1.8",
    "@types/qs": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "3.2.4",
    "esbuild-plugin-version-injector": "^1.2.1",
    "eslint": "^9.30.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-migrate-mongoose": "^3.8.7",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
